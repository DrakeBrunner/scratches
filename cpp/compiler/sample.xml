<!-- This is a sample source code -->
<?xml version="1.0" ?>
<code>
    <setq>
        <v>a</v>
        <o>+</o>
            <c>3</c>
            <o>-</o>
                <c>4</c>
                <c>5</c>
    </setq>

    <if>
        <cond>
            <o>==</o>
                <v>a</v>
                <c>2</c>
        </cond>

        <!-- true -->
        <body>
            <setq>
                <v>a</v>
                <c>1</c>
            </setq>
        </body>
        <!-- false -->
        <body>
            <setq>
                <v>a</v>
                <c>25</c>
            </setq>
        </body>
    </if>


    <loop type="do-while">
        <cond>
            <o>&lt;</o>
                <v>a</v>
                <c>20</c>
        </cond>

        <body>
            <setq>
                <v>a</v>
                <o>+</o>
                    <v>a</v>
                    <c>2</c>
            </setq>
        </body>
    </loop>
</code>
